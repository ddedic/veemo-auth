{
    "name": "ddedic/veemo-auth",
    "description": "Veemo Auth - core package. Login, registration, password resets, user activation, active, inactive, banned, throttling.",
    "keywords": [
        "veemo",
        "veemo-auth"
    ],
    "homepage": "https://github.com/ddedic/veemo-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Danijel Dedic",
            "email": "dedic.d@gmail.com",
            "homepage": "http://ddedic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "classmap": [
            "src/Exceptions/Exceptions.php"
        ],
        "psr-4": {
            "Veemo\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veemo\\Auth\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
